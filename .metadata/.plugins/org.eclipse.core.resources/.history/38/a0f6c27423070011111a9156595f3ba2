package repaso;

import java.util.Scanner;

public class Names {

	final static int TAM=5;  // NÃºmero de elementos del vector

	public static void main(String[] args) {
		String[] A = new String[TAM];

		// Pedimos 5 nombres y los almacenamos en A
		Scanner sc = new Scanner(System.in);
		System.out.println("Introduce los nombres...");

		for (int i=0; i<A.length; i++) {
			System.out.print("["+(i+1)+"] Introduce un nombre: ");
			A[i]=sc.nextLine();
		}	

		sc.close();

	}

}
