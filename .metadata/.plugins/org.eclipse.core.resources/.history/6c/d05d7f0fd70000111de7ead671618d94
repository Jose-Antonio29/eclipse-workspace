package strings;

public class StringTest {

	public static void main(String[] args) {
		// trim()
		String nombre = new String("   Harry Potteer   ");
		System.out.println(nombre.trim());
		System.out.println(nombre);
		nombre=nombre.trim();
		
		// replace()
		System.out.println(nombre.replace('e', 'x'));
		

	}

}
